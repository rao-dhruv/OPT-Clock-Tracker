{
    "manifest_version": 3,
    "name": "OPT Clock Tracker",
    "version": "1.0",
    "description": "Track and manage your 90 days of OPT unemployment for compliance.",
    "permissions": [
      "storage",
      "notifications"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon/icon16.png"
    },
    "icons": {
      "48": "icon/icon16.png"
    }
  }
  